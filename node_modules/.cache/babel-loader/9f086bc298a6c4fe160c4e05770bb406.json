te.cartList.forEach(item => item.checked = true)\n        }\n      }\n    }\n  }\n</script>\n\n<style scoped>\n  .bottom-bar {\n    height: 40px;\n    background-color: #eeeeee;\n    position: relative;\n    line-height: 40px;\n    display: flex;\n  }\n\n  .check-button {\n    width: 20px;\n    height: 20px;\n    line-height: 20px;\n    margin-right: 2px;\n  }\n\n  .check-content {\n    display: flex;\n    align-items: center;\n    margin-left: 5px;\n    width: 120px;\n  }\n\n  .price {\n    margin-left: 90px;\n    flex: 1;\n  }\n\n  .calculate {\n    width: 100px;\n    background: red;\n    color: #fff;\n    text-align: center;\n  }\n</style>\n"],"sourceRoot":"src/views/cart/childComps"}]}